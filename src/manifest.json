{
  "name": "chainmore",
  "description": "Web extension for chainmore",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_icon": {
       "19": "icons/19-action.png",
       "38": "icons/38-action.png"
    },
    "default_title": "掘金快捷分享"
 },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs"
  ],
  "commands": {
    "Run": {
      "suggested_key": {
        "default": "Ctrl+Shift+A",
        "mac": "Command+Shift+A"
      },
      "description": "Toggle feature foo"
    }
  }
}